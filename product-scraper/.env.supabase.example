# Supabase example
NODE_ENV=development
PORT=8080

# Copy your Supabase Postgres URI here (direct or pooled)
DATABASE_URL=postgresql://postgres:YOUR_PASSWORD@YOUR_HOST:5432/postgres?sslmode=require
# For pooled (PgBouncer, port 6543), you can use:
# DATABASE_URL=postgresql://postgres:YOUR_PASSWORD@YOUR_HOST:6543/postgres?pgbouncer=true&sslmode=require&connection_limit=1

SCRAPING_MODE=html
RATE_LIMIT_PER_DOMAIN_PER_SEC=0.5
MAX_RETRIES=3
HEADLESS=false
PROXY_URL=
USER_AGENTS_JSON=["Mozilla/5.0 (Windows NT 10.0; Win64; x64) AppleWebKit/537.36 (KHTML, like Gecko) Chrome/120.0.0.0 Safari/537.36"]

MAX_RECOMMENDATIONS=8
SEARCH_PAGES_TO_FETCH=1
PRICE_BAND_PERCENTAGE=25
LOG_LEVEL=info
